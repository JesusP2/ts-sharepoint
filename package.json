{
  "name": "ts-sharepoint",
  "version": "0.0.1",
  "description": "Typesafe sharepoint library",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "test": "tsx --experimental-test-coverage --test --env-file=.env ./__tests__/**",
    "test:watch": "tsx --test --env-file=.env ./__tests__/**",
    "lint": "eslint index.ts",
    "format": "prettier --write --experimental-ternaries ./src",
    "build": "tsc && tsup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.3",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
